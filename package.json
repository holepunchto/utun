{
  "name": "utun",
  "version": "2.1.0",
  "description": "Make TUN devices",
  "exports": {
    ".": "./index.js",
    "./package": "./package.json"
  },
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "events": {
      "bare": "bare-events",
      "default": "events"
    }
  },
  "files": [
    "index.js",
    "apple.h",
    "linux.h",
    "binding.c",
    "binding.js",
    "CMakeLists.txt",
    "prebuilds"
  ],
  "keywords": [
    "utun",
    "tun",
    "vpn"
  ],
  "addon": true,
  "scripts": {
    "test": "sudo node test.js",
    "lint": "standard",
    "debug": "sudo node inspect test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/utun.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/utun/issues"
  },
  "homepage": "https://github.com/holepunchto/utun#readme",
  "dependencies": {
    "bare-subprocess": "^5.0.0",
    "require-addon": "^1.0.6",
    "which-runtime": "^1.2.0"
  },
  "devDependencies": {
    "bare-compat-napi": "^1.0.0",
    "brittle": "^3.7.0",
    "cmake-bare": "^1.1.15",
    "cmake-napi": "^1.0.7",
    "cmake-npm": "^1.0.2",
    "ip-packet": "^2.2.1",
    "standard": "^17.1.0",
    "udx-native": "^1.14.1"
  }
}
